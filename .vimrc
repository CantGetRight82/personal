execute pathogen#infect()
syntax on
filetype plugin indent on
set number
set showcmd
set ruler

set hlsearch
set incsearch
set background=dark
colorscheme solarized

set backupdir=~/.vim/backup/
set directory=~/.vim/swap/
set undodir=~/.vim/undo/

set autoindent
set tabstop=4 softtabstop=0 noexpandtab shiftwidth=4
